{
    // Enforcing options
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true,

    // Relaxing options
    //"esnext": true,

    // Environments
    "browser": true,
    "devel": true,

    // Custom globals
    "globals": {
        // RequireJS
        "define": true,
        "require": true,

        // File API
        "File": true,
        "FileReader": true
    }
}


